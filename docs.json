{
    "openapi": "3.0.2",
    "info": {"title": "Hello!", "version": "0.1.0"},
    "paths": {
        "/api/add/": {
            "post": {
                "summary": "Add Document",
                "description": "Adding a new document to the database.",
                "operationId": "add_document_api_add__post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {"$ref": "#/components/schemas/DocumenIn"}
                        }
                    },
                    "required": true,
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {"$ref": "#/components/schemas/DocumenOut"}
                            }
                        },
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        },
                    },
                },
            }
        },
        "/api/delete/": {
            "delete": {
                "summary": "Delete Document",
                "description": "Removing a document from the database.",
                "operationId": "delete_document_api_delete__delete",
                "parameters": [
                    {
                        "required": true,
                        "schema": {"title": "Doc Id", "type": "integer"},
                        "name": "doc_id",
                        "in": "query",
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SuccessResponse"
                                }
                            }
                        },
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        },
                    },
                },
            }
        },
        "/api/search/": {
            "get": {
                "summary": "Search Document",
                "description": "Searching for the first 20 entries that contain a pattern",
                "operationId": "search_document_api_search__get",
                "parameters": [
                    {
                        "required": true,
                        "schema": {"title": "Pattern", "type": "string"},
                        "name": "pattern",
                        "in": "query",
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "title": "Response Search Document Api Search  Get",
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/DocumenFullOut"
                                    },
                                }
                            }
                        },
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        },
                    },
                },
            }
        },
    },
    "components": {
        "schemas": {
            "DocumenFullOut": {
                "title": "DocumenFullOut",
                "required": ["rubrics", "text", "doc_id", "created_date"],
                "type": "object",
                "properties": {
                    "rubrics": {
                        "title": "Rubrics",
                        "type": "array",
                        "items": {"type": "string"},
                    },
                    "text": {"title": "Text", "type": "string"},
                    "doc_id": {"title": "Doc Id", "type": "integer"},
                    "created_date": {
                        "title": "Created Date",
                        "type": "string",
                        "format": "date-time",
                    },
                },
            },
            "DocumenIn": {
                "title": "DocumenIn",
                "required": ["rubrics", "text"],
                "type": "object",
                "properties": {
                    "rubrics": {
                        "title": "Rubrics",
                        "type": "array",
                        "items": {"type": "string"},
                    },
                    "text": {"title": "Text", "type": "string"},
                },
            },
            "DocumenOut": {
                "title": "DocumenOut",
                "required": ["rubrics", "text", "doc_id", "created_date"],
                "type": "object",
                "properties": {
                    "rubrics": {
                        "title": "Rubrics",
                        "type": "array",
                        "items": {"type": "string"},
                    },
                    "text": {"title": "Text", "type": "string"},
                    "doc_id": {"title": "Doc Id", "type": "integer"},
                    "created_date": {
                        "title": "Created Date",
                        "type": "string",
                        "format": "date-time",
                    },
                },
            },
            "HTTPValidationError": {
                "title": "HTTPValidationError",
                "type": "object",
                "properties": {
                    "detail": {
                        "title": "Detail",
                        "type": "array",
                        "items": {"$ref": "#/components/schemas/ValidationError"},
                    }
                },
            },
            "SuccessResponse": {
                "title": "SuccessResponse",
                "required": ["status_code", "message"],
                "type": "object",
                "properties": {
                    "status_code": {"title": "Status Code", "type": "integer"},
                    "message": {"title": "Message", "type": "string"},
                },
            },
            "ValidationError": {
                "title": "ValidationError",
                "required": ["loc", "msg", "type"],
                "type": "object",
                "properties": {
                    "loc": {
                        "title": "Location",
                        "type": "array",
                        "items": {"anyOf": [{"type": "string"}, {"type": "integer"}]},
                    },
                    "msg": {"title": "Message", "type": "string"},
                    "type": {"title": "Error Type", "type": "string"},
                },
            },
        }
    },
}
